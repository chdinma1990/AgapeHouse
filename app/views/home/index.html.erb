                                        <div class="latest-sermon-video fw-video">
                                            <iframe src="https://www.facebook.com/plugins/video.php?href=https%3A%2F%2Fwww.facebook.com%2F100017105711758%2Fvideos%2F106968369883353%2F&show_text=0&width=269" width="269" height="476" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true" allowFullScreen="true"></iframe>
                                        </div>
                                        <div class="latest-sermon-content">
                                            <div class="meta-data"><h4><a href="#"><%= video.title %></a></h4></div>
                                            <p><%= video.description %></p>
                                        </div>
                                    </li>
                                <% end %>
                            <% end %>
                           <!-- <li><a class= "basic-link" href="https://www.youtube.com/user/ShekinahGloryFire/videos" target="_blank">Watch More Sermons</a></li> -->
                        </ul>
                    </div>
                </div>
                <div class="col-md-7 col-sm-7">
                    <div class="element-block events-listing">
