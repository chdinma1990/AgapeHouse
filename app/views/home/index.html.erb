   <div class="upcoming-event-bar event-dynamic">
    <div class="container">
        <div class="row">
            <% unless date_of_next("Sunday").strftime("%d %b, %Y") == DateTime.parse("20 AUG, 2017").strftime("%d %b, %Y") %>
            <div class="col-md-6 col-sm-6">
                <span class="label label-primary">Next Event</span>
                <h3><a href="#" class="event-title">Sunday Celebration Service</a></h3>
                <h4 style="margin:0px;">
                <span class="meta-data">On <span class="event-date"><%= date_of_next("Sunday").strftime("%d %b, %Y") %></span> at <span class="event-time">11:00 AM</span> at</span><span class="meta-data event-location"> <span class="event-location-address">6400 Mission Rd, Prairie Village, KS 66204</span></span>
                </h4>
            </div>
            <div class="col-md-4 col-sm-4">
                <div id="counter" class="counter clearfix" data-date="<%= date_of_next("Sunday").strftime("%d %b, %Y") %>">
                    <div class="timer-col"> <span id="days"></span> <span class="timer-type">Days</span> </div>
                    <div class="timer-col"> <span id="hours"></span> <span class="timer-type">Hours</span> </div>
                    <div class="timer-col"> <span id="minutes"></span> <span class="timer-type">Minutes</span> </div>
                    <div class="timer-col"> <span id="seconds"></span> <span class="timer-type">Seconds</span> </div>
                </div>
            </div>
            <% else %>
            <div class="col-md-6 col-sm-6">
                <span class="label label-primary">Upcoming Event</span>
                <h3><a href="#" class="event-title">Bible Study</a></h3>
                <h4 style="margin:0px;">
                <span class="meta-data">On <span class="event-date">WED 23 AUG, 2017</span> at
                <span class="meta-data event-location">
                    <span class="event-location-address">Teleconference</span>
                </span>
                </h4>
            </div>
            <div class="col-md-4 col-sm-4">
                <div id="counter" class="counter clearfix" data-date="<%= date_of_next("Friday").strftime("%d %b, %Y") %>">
                    <div class="timer-col"> <span id="days"></span> <span class="timer-type">Days</span> </div>
                    <div class="timer-col"> <span id="hours"></span> <span class="timer-type">Hours</span> </div>
                    <div class="timer-col"> <span id="minutes"></span> <span class="timer-type">Minutes</span> </div>
                    <div class="timer-col"> <span id="seconds"></span> <span class="timer-type">Seconds</span> </div>
                </div>
            </div>
            <% end %>
            <div class="col-md-2 text-align-right">
                <ul class="action-buttons">
                    <li title="Share event"><a href="#" data-trigger="focus" data-placement="top" data-content="" data-toggle="popover" data-original-title="Share Event" class="event-share-link"><i class="icon-share"></i></a></li>
                    <li title="Get directions" class="hidden-xs"><a href="#" class="cover-overlay-trigger event-direction-link"><i class="icon-compass"></i></a></li>
<!--                     <li title="Contact event manager"><a href="#" data-toggle="modal" data-target="#Econtact" class="event-contact-link"><i class="icon-mail"></i></a></li> -->
                </ul>
                <!-- <a href="#" class="btn btn-primary btn-sm event-tickets event-register-button">Register</a> -->
            </div>
        </div>
    </div>
</div>
<!-- Start Body Content -->
