    <div class="upcoming-event-bar event-dynamic">
    <div class="container">
        <div class="row">
            <% unless date_of_next("Sunday").strftime("%d %b, %Y") == DateTime.parse("26 MAR, 2017").strftime("%d %b, %Y") %>
            <div class="col-md-6 col-sm-6">
               <span class="label label-primary" style="font-weight: bold;">Next Event:</span> 
                <h3><a href="#" class="event-title" style="font-weight: bold;"> Sunday Service</a></h3>
                <h4 style="margin:0px;">
                <span class="meta-data" style="font-weight: bold;">On <span class="event-date"><%= date_of_next("Sunday").strftime("%d %b, %Y") %></span> at <span class="event-time">10:00 AM</span> at</span><span class="meta-data event-location"> <span class="event-location-address"></span> <span>8155 Parallel Parkway, Kansas City, KANSAS 66112</span>
                </h4>
            </div>
            <div class="col-md-4 col-sm-4">
                <div id="counter" class="counter clearfix" data-date="<%= date_of_next("Sunday").strftime("%d %b, %Y") %>">
                    <div class="timer-col"> <span id="days"></span> <span class="timer-type">Days</span> </div>
                    <div class="timer-col"> <span id="hours"></span> <span class="timer-type">Hours</span> </div>
                    <div class="timer-col"> <span id="minutes"></span> <span class="timer-type">Minutes</span> </div>
                    <div class="timer-col"> <span id="seconds"></span> <span class="timer-type">Seconds</span> </div>
                </div>
            </div>
            <% else %>
            
            <div class="col-md-4 col-sm-4">
                <div id="counter" class="counter clearfix" data-date="<%= date_of_next("Sunday").strftime("%d %b, %Y") %>">
                    <div class="timer-col"> <span id="days"></span> <span class="timer-type">Days</span> </div>
                    <div class="timer-col"> <span id="hours"></span> <span class="timer-type">Hours</span> </div>
                    <div class="timer-col"> <span id="minutes"></span> <span class="timer-type">Minutes</span> </div>
                    <div class="timer-col"> <span id="seconds"></span> <span class="timer-type">Seconds</span> </div>
                </div>
            </div>
            <% end %>
            <div class="col-md-2 text-align-right">
                <ul class="action-buttons">
                <li title="Share event"><a href="http://www.rccgagapehouseks.org/" data-trigger="focus" data-placement="top" data-content="" data-toggle="popover" data-original-title="Share Event" class="event-share-link"><i class="fa fa-share-alt"></i></a></li>
