                            <h3 class="element-title">Upcoming Services</h3>
                            <hr class="sm">
                        </div>
                        <div class="events-listing-content">

                            <%= render partial: 'event_listing', :locals => {:day => Date.today} %>
                            <%= render partial: 'event_listing', :locals => {:day => 1.day.from_now} %>
                            <%= render partial: 'event_listing', :locals => {:day => 2.days.from_now} %>
                            <%= render partial: 'event_listing', :locals => {:day => 3.days.from_now} %>
                            <%= render partial: 'event_listing', :locals => {:day => 4.days.from_now} %>
                            <%= render partial: 'event_listing', :locals => {:day => 5.days.from_now} %>
                            <%= render partial: 'event_listing', :locals => {:day => 6.days.from_now} %>
                            <%= render partial: 'event_listing', :locals => {:day => 7.days.from_now} %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="spacer-30"></div>
    </div>
</div>
